# ---------- locale/en/api.po ----------
msgid "api.announcements.400.contextsNotMatched"
msgstr "The announcement you requested is not part of this podcast."

msgid "api.emails.403.disabled"
msgstr "The email notification feature has not been enabled for this podcast."

msgid "api.submissions.400.wrongContext"
msgstr "The submission you requested is not in this podcast."

msgid "api.submissions.403.cantChangeContext"
msgstr "You can not change the podcast of a submission."

msgid "api.submissions.403.unpublishedIssues"
msgstr "You do not have permission to view unpublished seasons."

msgid "api.contexts.403.requestedDisabledContexts"
msgstr "You can only view podcasts that have been enabled."

msgid "api.contexts.403.notAllowed"
msgstr "You do not have permission to view this podcast."

msgid "api.contexts.403.contextsDidNotMatch"
msgstr ""
"You can not view or edit this podcast unless you make a request to the "
"podcast's API or the site-wide API."

msgid "api.contexts.403.requiresContext"
msgstr "You can not edit this podcast from the site-wide API."

msgid "api.contexts.403.notAllowedEdit"
msgstr "You do not have permission to edit this podcast."

msgid "api.contexts.403.notAllowedDelete"
msgstr "You do not have permission to delete this podcast."

msgid "api.contexts.404.contextNotFound"
msgstr "The podcast you requested was not found."

msgid "api.dois.403.contextsNotMatched"
msgstr "The DOI you requested is not part of this podcast."

msgid "api.dois.400.noRegistrationAgencyConfigured"
msgstr "No DOI registration agency has been configured for this podcast."

msgid "api.emailTemplates.403.notAllowedChangeContext"
msgstr ""
"You do not have permission to move this email template to another podcast."

msgid "api.publications.403.contextsDidNotMatch"
msgstr "The publication that you requested is not part of this podcast."
