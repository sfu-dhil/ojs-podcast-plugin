# ---------- lib/pkp/locale/en/submission.po ----------
msgid "metadata.property.displayName.article-title"
msgstr "Episode Title"

msgid "metadata.property.validationMessage.article-title"
msgstr "Please enter a valid episode title."

msgid "metadata.property.displayName.issue"
msgstr "Season"

msgid "metadata.property.validationMessage.issue"
msgstr "Please enter a valid season."

msgid "metadata.property.validationMessage.publication-type"
msgstr "Please make sure that you always set a publication type. Currently supported values are book (for books or book chapters) and podcast (for podcast episodes). Otherwise the citation output formatting will not work correctly."

msgid "dashboard.toBePublishedInIssue"
msgstr "To be published in season {$issue}"

msgid "dashboard.noAccessBeingAuthor"
msgstr "You cannot access this submission as a Podcast Manager since you are the author. To view it, go to \"My Submissions\""

msgid "dashboard.noAccessBeingReviewer"
msgstr "You cannot access this submission as a Podcast Manager since you are the reviewer. To view it, go to \"Review Assignments\""

# ---------- locale/en/submission.po ----------
msgid "submission.upload.selectComponent"
msgstr "Select episode component"

msgid "submission.title"
msgstr "Episode Title"

msgid "submission.submit.noContext"
msgstr "This submission's podcast could not be found."

msgid "submission.submit.whatNext.description"
msgstr ""
"The Podcast has been notified of your submission, and you've been emailed a "
"confirmation for your records.  Once the editor has reviewed the submission, "
"they will contact you."

msgid "grid.action.galleyInIssueEntry"
msgstr "Galley appears in season entry"

msgid "submission.event.articleGalleyMadeAvailable"
msgstr "The episode galley \"{$galleyFormatName}\" is made available."

msgid "submission.event.articleGalleyMadeUnavailable"
msgstr "The episode galley \"{$galleyFormatName}\" is made unavailable."

msgid "grid.issueEntry.availableGalley.removeMessage"
msgstr ""
"<p>This galley will <em>no longer be made available</em> to listeners.</p>"

msgid "grid.issueEntry.availableGalley.message"
msgstr "<p>This galley will <em>be made available</em> to listeners.</p>"

msgid "submission.submit.upload.description"
msgstr ""
"Upload files associated with this submission, including the episode, "
"multimedia, data sets, artwork, etc."

msgid "submission.event.issueMetadataUpdated"
msgstr "The submission's season metadata was updated."

msgid "submission.upload.fileContents"
msgstr "Episode Component"

msgid "submission.metadataDescription"
msgstr ""
"These specifications are based on the Dublin Core metadata set, an "
"international standard used to describe podcast content."

msgid "publication.assignToissue"
msgstr "Assign to Season"

msgid "publication.changeIssue"
msgstr "Change Season"

msgid "publication.datePublished.description"
msgstr ""
"The publication date will be set automatically when the season is published. "
"Do not enter a publication date unless the episode was previously published "
"elsewhere and you need to backdate it."

msgid "publication.invalidIssue"
msgstr "The season for this publication could not be found."

msgid "publication.required.issue"
msgstr ""
"The publication must be assigned to an season before it can be published."

msgid "publication.unscheduledIn"
msgstr "This has not been scheduled for publication in an season."

msgid "publication.selectIssue"
msgstr "Select an season to schedule for publication"

msgid "publication.copyrightYearBasis.issueDescription"
msgstr ""
"The copyright year will be set automatically when this is published in an "
"season."

msgid "doi.submission.issueMissing.publication"
msgstr ""
"Could not create a DOI for the following submission: {$pubObjectTitle}. The "
"submission must be assigned to an season before a DOI can be generated."

msgid "doi.submission.issueMissing.representation"
msgstr ""
"Could not create a DOI for the {$pubObjectTitle} galley for the following "
"submission: {$itemTitle}. The submission must be assigned to an season before "
"a DOI can be generated."

msgid "doi.submission.incorrectContext"
msgstr ""
"Could not create a DOI for the following submission: {$pubObjectTitle}. It "
"does not exist in the current podcast context."

msgid "submission.copyright.description"
msgstr ""
"Please read and understand the copyright terms for submissions to this "
"podcast."

msgid "submission.wizard.notAllowed.description"
msgstr ""
"You are not allowed to submit to this podcast because authors must be "
"registered by the editorial staff. If you believe this is an error, please "
"contact <a href='mailto:{$email}'>{$name}</a>."

msgid "submission.wizard.noSectionAllowed.description"
msgstr ""
"You are not allowed to submit to this podcast because submissions to all "
"sections of this podcast have been deactivated or restricted. If you believe "
"this is an error, please contact <a href='mailto:{$email}'>{$name}</a>."

msgid "submission.submit.form.titleRequired"
msgstr "Please enter the title of your episode."

msgid "submission.submit.form.abstractRequired"
msgstr "Please enter your episode's abstract."
